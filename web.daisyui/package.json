{
    "name": "@houseofwolves/serverlesslaunchpad.web.daisyui",
    "version": "1.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "local:watch": "vite --mode moto",
        "local:development": "vite --mode development",
        "local:staging": "vite --mode staging",
        "build": "tsc && vite build",
        "build:production": "tsc && vite build --mode production",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "preview": "vite preview",
        "test": "vitest",
        "test:watch": "vitest --watch"
    },
    "dependencies": {
        "@houseofwolves/serverlesslaunchpad.types": "*",
        "@houseofwolves/serverlesslaunchpad.web.commons": "*",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.22.0",
        "aws-amplify": "^6.14.4",
        "date-fns": "^4.1.0",
        "zod": "^4.1.11",
        "lucide-react": "^0.462.0",
        "react-hot-toast": "^2.4.1",
        "clsx": "^2.1.0"
    },
    "devDependencies": {
        "@types/react": "^18.2.43",
        "@types/react-dom": "^18.2.17",
        "@typescript-eslint/eslint-plugin": "^6.14.0",
        "@typescript-eslint/parser": "^6.14.0",
        "@vitejs/plugin-react": "^4.2.1",
        "daisyui": "^4.12.14",
        "tailwindcss": "^3.4.1",
        "autoprefixer": "^10.4.17",
        "postcss": "^8.4.33",
        "eslint": "^8.55.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.5",
        "typescript": "^5.2.2",
        "vite": "^7.1.9",
        "vitest": "^3.2.4"
