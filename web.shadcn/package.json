        "@types/react": "^18.2.43",
        "@types/react-dom": "^18.2.17",
        "@vitejs/plugin-react": "^5.0.4",
        "autoprefixer": "^10.4.17",
        "eslint": "^9.18.0",
        "eslint-plugin-react-hooks": "^5.1.0",
        "eslint-plugin-react-refresh": "^0.4.17",
        "globals": "^15.14.0",
        "nodemon": "^3.1.10",
        "postcss": "^8.4.33",
        "prettier": "^3.2.5",
        "tailwindcss": "^3.4.1",
        "typescript": "^5.2.2",
        "typescript-eslint": "^8.21.0",
        "vite": "^7.0.0",
        "vitest": "^3.2.4"
    }
