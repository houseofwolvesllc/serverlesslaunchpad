{
    "name": "@houseofwolves/pubdesk.framework",
    "version": "1.0.0",
    "author": "Keith Garcia",
    "description": "Library 3rd party implementations for the Pubdesk platform.",
    "license": "UNLICENSED",
    "private": true,
    "main": "src/index.ts",
    "type": "module",
    "scripts": {
        "process": "npx tsx ./src/test.ts",
        "clean": "rimraf coverage build tmp",
        "prebuild": "npm run lint",
        "build": "tsc -p tsconfig.json",
        "build:watch": "tsc -w -p tsconfig.json",
        "build:release": "npm run clean && tsc -p tsconfig.release.json",
        "lint": "eslint . --ext .ts --ext .mts",
        "test": "vitest --coverage",
        "prettier": "prettier --config .prettierrc.json --write .",
        "test:watch": "vitest --watch"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.712.0",
        "@houseofwolves/serverlesslaunchpad.core": "*",
        "@houseofwolves/serverlesslaunchpad.types": "*",
        "aws-amplify": "^6.13.4",
        "aws-jwt-verify": "^5.0.0",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@types/node": "~18",
        "@typescript-eslint/eslint-plugin": "~5.44",
        "@typescript-eslint/parser": "~5.44",
        "@vitest/coverage-c8": "^0.27.3",
        "eslint": "~8.28",
        "eslint-config-prettier": "~8.5",
        "eslint-import-resolver-typescript": "^3.7.0",
        "eslint-plugin-prettier": "^5.2.3",
        "eslint-plugin-vitest": "^0.0.29",
        "prettier": "^3.4.2",
        "rimraf": "~3.0",
        "tslib": "^2.8.1",
        "tsx": "^4.16.5",
        "vitest": "^0.27.2"
    }
}
