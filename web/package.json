{
    "name": "mantine-cognito",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "local": "vite --mode moto 2>&1 | tee ../logs/web.log",
        "local:watch": "nodemon --watch ../core/dist --watch ../framework/dist --watch ../commons/dist --exec \"vite --mode moto\" --delay 1 2>&1 | tee ../logs/web.log",
        "local:development": "vite --mode development",
        "local:staging": "vite --mode staging",
        "local:production": "vite --mode production",
        "build": "tsc && vite build",
        "build:development": "tsc && vite build --mode development",
        "build:staging": "tsc && vite build --mode staging",
        "build:production": "tsc && vite build --mode production",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "preview": "vite preview",
        "test": "vitest run",
        "test:watch": "vitest"
    },
    "dependencies": {
        "@mantine/core": "^7.13.4",
        "@mantine/form": "^7.13.4",
        "@mantine/hooks": "^7.13.4",
        "@mantine/notifications": "^7.13.4",
        "@tabler/icons-react": "^2.47.0",
        "aws-amplify": "^6.14.4",
        "date-fns": "^4.1.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.22.0",
        "zod": "^4.1.11"
    },
    "devDependencies": {
        "@types/react": "^18.2.43",
        "@types/react-dom": "^18.2.17",
        "@typescript-eslint/eslint-plugin": "^6.14.0",
        "@typescript-eslint/parser": "^6.14.0",
        "@vitejs/plugin-react": "^5.0.4",
        "eslint": "^8.55.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.5",
        "nodemon": "^3.1.10",
        "postcss": "^8.4.32",
        "postcss-preset-mantine": "^1.17.0",
        "postcss-simple-vars": "^7.0.1",
        "prettier": "^3.2.5",
        "typescript": "^5.2.2",
        "vite": "^7.1.9",
        "vitest": "^3.2.4"
    }
}
