{
    "name": "@houseofwolves/serverlesslaunchpad.web.mantine",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "local": "vite --mode moto 2>&1 | tee ../logs/web.log",
        "local:watch": "nodemon --watch ../core/dist --watch ../framework/dist --watch ../types/dist --exec \"vite --mode moto\" --delay 1 2>&1 | tee ../logs/web.log",
        "local:development": "vite --mode development",
        "local:staging": "vite --mode staging",
        "local:production": "vite --mode production",
        "build": "tsc && vite build",
        "build:development": "tsc && vite build --mode development",
        "build:staging": "tsc && vite build --mode staging",
        "build:production": "tsc && vite build --mode production",
        "lint": "eslint .",
        "preview": "vite preview",
        "test": "vitest run",
        "test:watch": "vitest"
    },
    "dependencies": {
        "@houseofwolves/serverlesslaunchpad.types": "*",
        "@houseofwolves/serverlesslaunchpad.web.commons": "*",
        "@houseofwolves/serverlesslaunchpad.web.commons.react": "*",
        "@mantine/core": "^8.3.6",
        "@mantine/form": "^8.3.6",
        "@mantine/hooks": "^8.3.6",
        "@mantine/modals": "^8.3.6",
        "@mantine/notifications": "^8.3.6",
        "@tabler/icons-react": "^2.47.0",
        "aws-amplify": "^6.14.4",
        "date-fns": "^4.1.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.22.0",
        "zod": "^4.1.11"
    },
    "devDependencies": {
        "@eslint/js": "^9.18.0",
        "@types/react": "^18.2.43",
        "@types/react-dom": "^18.2.17",
        "@vitejs/plugin-react": "^5.0.4",
        "eslint": "^9.18.0",
        "eslint-plugin-react-hooks": "^5.1.0",
        "eslint-plugin-react-refresh": "^0.4.17",
        "globals": "^15.14.0",
        "nodemon": "^3.1.10",
        "postcss": "^8.4.32",
        "postcss-preset-mantine": "^1.18.0",
        "postcss-simple-vars": "^7.0.1",
        "prettier": "^3.2.5",
        "typescript": "^5.2.2",
        "typescript-eslint": "^8.21.0",
        "vite": "^7.0.0",
        "vitest": "^3.2.4"
    }
}
